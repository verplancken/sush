      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.mensajes ){",
        "\treturn {",
        "\t\t//map mensajes variable to expected task contract input",
        "\t\tmensajesInput: {",
        "\t\t\tclave: $data.mensajes.clave !== undefined ? $data.mensajes.clave : null,",
        "\t\t\tmensaje: $data.mensajes.mensaje !== undefined ? $data.mensajes.mensaje : null,",
        "\t\t\tfecha: $data.mensajes.fecha !== undefined ? $data.mensajes.fecha : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
